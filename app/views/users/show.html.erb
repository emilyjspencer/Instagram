<h1 align="center"><%= @user.username %>'s feed</h1>
<div class="row">
 <div class="col-md-4 col-md-offset-5 center">
  <%= gravatar_for @user, size: 150 %>
 </div>
</div>
<h4 align="center"><%= @user.username %>'s articles</h4>
<%= render 'shared/post', obj: @user.posts %>
<div align="center">
  <%= will_paginate @user_posts %>
</div>